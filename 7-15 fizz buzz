/* 
7/13/15
Loops can be time and processor intensive. Inputing a number into a prompt should return a single value for the number; 
and not have to loop through all possibilities.
*/

var num = Number(prompt("please enter a number")); 
if (num % 3 == 0 && num % 5 == 0) { 
  console.log('fizzbuzz');
}
if (num % 3 == 0) { 
  console.log('fizz');
}
if (num % 5 == 0) { 
  console.log('buzz');
}
else (console.log('boring'));
